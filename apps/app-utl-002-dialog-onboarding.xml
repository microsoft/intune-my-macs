  <PreInstallScript>apps/app-utl-002-dialog-onboarding_pre.sh</PreInstallScript>
  <PostInstallScript>apps/app-utl-002-dialog-onboarding_post.sh</PostInstallScript>
  </Package>
</MacIntuneManifest>
